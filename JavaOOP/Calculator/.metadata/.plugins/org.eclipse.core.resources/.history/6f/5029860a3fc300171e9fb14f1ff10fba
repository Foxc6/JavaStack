
public class Calculator implements java.io.Serializable {
	public Calculator() {
	}
	public int setOperandOne(int operandOne) {
		return operandOne;
	}
	
	public int setOperandTwo(int operandTwo) {
		return operandTwo;
	}
	
	public String setOperation(String operation) {
		return operation;
	}
	
	public int performOperation(){
		return this.setOperandOne();
	}
	
}
